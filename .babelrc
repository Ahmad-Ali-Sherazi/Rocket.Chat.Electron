{
  "exclude": "node_modules/**",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "electron": 5
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-function-bind",
    "@babel/plugin-proposal-class-properties",
  ],
}
